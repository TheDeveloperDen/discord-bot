{
	"name": "devdenbot",
	"version": "10.1.0",
	"type": "module",
	"main": "src/index.ts",
	"engines": {
		"node": "22"
	},
	"scripts": {
		"version": "echo $npm_package_version",
		"lint": "bunx biome check --fix",
		"typecheck": "bunx tsc --noEmit --build tsconfig.json",
		"typecheck:prod": "bunx tsc --noEmit -p tsconfig.production.json",
		"watch-dev": "bun --watch src/index.ts",
		"start": "bun --trace-warnings src/index.ts",
		"start:prod": "bun run lint && bun run typecheck:prod && NODE_ENV=production bun run start",
		"prepare": "bunx husky || true",
		"test": "bun test",
		"test:retry": "scripts/bun-test-retry.sh"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"keywords": [],
	"author": "Developer Den",
	"license": "MIT",
	"dependencies": {
		"@napi-rs/canvas": "^0.1.82",
		"@sentry/bun": "^10.26.0",
		"@sentry/cli": "^2.58.2",
		"@sequelize/core": "^7.0.0-alpha.47",
		"@sequelize/postgres": "^7.0.0-alpha.47",
		"@sequelize/sqlite3": "^7.0.0-alpha.47",
		"@types/fluent-ffmpeg": "^2.1.28",
		"bufferutil": "^4.0.9",
		"discord.js": "^14.25.1",
		"djs-slash-helper": "git+https://github.com/bristermitten/djs-slash-helper.git#97726a356be094f4d187637734b116e4e1c89be5",
		"expiry-map": "^2.0.0",
		"fluent-ffmpeg": "^2.1.3",
		"handlebars": "^4.7.8",
		"hot-takes": "git+https://github.com/bristermitten/hot-takes.git#ec379b16fbba0842938ffb77b35abcf3690f2ddd",
		"lru-cache": "^11.2.2",
		"mariadb": "^3.4.5",
		"node-fetch": "^3.3.2",
		"node-schedule": "^2.1.1",
		"opentype.js": "^1.3.4",
		"pg": "^8.16.3",
		"pg-hstore": "^2.3.4",
		"pg-types": "^4.1.0",
		"reflect-metadata": "^0.2.2",
		"sqlite3": "^5.1.7",
		"string-similarity": "^4.0.4",
		"tslib": "^2.8.1",
		"winston": "^3.18.3",
		"yaml": "^2.8.1"
	},
	"trustedDependencies": [
		"@sentry/cli",
		"bufferutil",
		"sqlite3",
		"utf-8-validate"
	],
	"repository": "https://github.com/TheDeveloperDen/DevDenBot.git",
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@sentry/types": "^10.26.0",
		"@sinonjs/fake-timers": "^15.0.0",
		"@types/bun": "^1.3.3",
		"@types/handlebars": "^4.1.0",
		"@types/node": "^24.10.1",
		"@types/node-schedule": "^2.1.8",
		"@types/opentype.js": "^1.3.8",
		"@types/pg": "^8.15.6",
		"@types/sequelize": "^6.12.0",
		"@types/sinonjs__fake-timers": "^15.0.1",
		"@types/sqlite3": "^5.1.0",
		"@types/string-similarity": "^4.0.2",
		"@types/validator": "^13.15.10",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"nodemon": "^3.1.11",
		"ts-node": "^10.9.2",
		"ts-prune": "^0.10.3",
		"ts-standard": "^12.0.2",
		"typescript": "^5.9.3"
	}
}
