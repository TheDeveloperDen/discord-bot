{
  "name": "devdenbot",
  "version": "8.4.5",
  "type": "module",
  "main": "src/index.ts",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "version": "echo $npm_package_version",
    "lint": "bunx eslint 'src/**/*.ts' --fix",
    "build": "tsc --build --clean tsconfig.json && tsc --build tsconfig.json",
    "build-prod": "tsc -p tsconfig.production.json && ([ -f bin/Config.prod.js ] && mv -f bin/Config.prod.js bin/Config.js) || true",
    "watch-dev": "bun --watch src/index.ts",
    "start-built": "bun --import ./instrument.mjs --trace-warnings bin/index.js",
    "build-and-run": "bun run build && bun run start-built",
    "build-and-run-prod": "bun run lint && bun run build-prod && bun run start-built",
    "sentry:sourcemaps": "bunx sentry-cli sourcemaps inject --org developer-den-59 --project discord-bot ./bin && bunx sentry-cli sourcemaps upload --org developer-den-59 --project discord-bot ./bin"
  },
  "keywords": [],
  "author": "Developer Den",
  "license": "MIT",
  "dependencies": {
    "@microsoft/eslint-formatter-sarif": "3.1.0",
    "@sentry/bun": "^10.1.0",
    "@sentry/cli": "^2.50.2",
    "@sentry/node": "^8.55.0",
    "@sentry/profiling-node": "^8.55.0",
    "@sequelize/core": "^7.0.0-alpha.46",
    "@sequelize/postgres": "^7.0.0-alpha.46",
    "@sequelize/sqlite3": "^7.0.0-alpha.46",
    "bufferutil": "^4.0.9",
    "canvas": "^3.1.2",
    "discord.js": "^14.21.0",
    "djs-slash-helper": "git+https://github.com/bristermitten/djs-slash-helper.git#bf71648b3232ea9c308b8ee79a3f4a885fc1e92b",
    "expiry-map": "^2.0.0",
    "lru-cache": "^11.1.0",
    "mariadb": "^3.4.5",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "opentype.js": "^1.3.4",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pg-types": "^4.1.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "string-similarity": "^4.0.4",
    "tslib": "^2.8.1",
    "utf-8-validate": "^6.0.5",
    "winston": "^3.17.0",
    "yaml": "^2.8.0",
    "zlib-sync": "^0.1.10"
  },
  "repository": "https://github.com/TheDeveloperDen/DevDenBot.git",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@sentry/types": "^8.55.0",
    "@types/node": "^22.17.0",
    "@types/node-schedule": "^2.1.8",
    "@types/opentype.js": "^1.3.8",
    "@types/pg": "^8.15.5",
    "@types/sequelize": "^4.28.20",
    "@types/sqlite3": "^3.1.11",
    "@types/string-similarity": "^4.0.2",
    "@types/validator": "^13.15.2",
    "eslint": "^9.32.0",
    "globals": "^15.15.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "ts-standard": "^12.0.2",
    "typescript": ">=4.8.4 <5.9.0",
    "typescript-eslint": "^8.38.0"
  }
}
