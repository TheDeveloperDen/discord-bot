{
  "name": "devdenbot",
  "version": "3.16.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "version": "echo $npm_package_version",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "build": "tsc --build --clean tsconfig.json && tsc --build tsconfig.json",
    "build-prod": "tsc -p tsconfig.production.json && ([ -f bin/Config.prod.js ] && mv -f bin/Config.prod.js bin/Config.js) || true",
    "start": "nodemon --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "dev": "node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "start-built": "node --prof --inspect=0.0.0.0:9229 --trace-warnings bin/index.js",
    "build-and-run": "yarn lint && yarn build && yarn start-built",
    "build-and-run-prod": "yarn lint && yarn build-prod && yarn start-built"
  },
  "keywords": [],
  "author": "Developer Den",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "@sentry/node": "^7.38.0",
    "@sentry/tracing": "^7.38.0",
    "@types/validator": "^13.7.14",
    "bufferutil": "^4.0.7",
    "canvas": "^2.9.3",
    "discord-api-types": "^0.37.35",
    "discord.js": "^14.8.0",
    "djs-slash-helper": "^2.0.0",
    "erlpack": "discord/erlpack",
    "expiry-map": "^2.0.0",
    "mariadb": "^3.0.1",
    "node-fetch": "^3.2.10",
    "opentype.js": "^1.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.3",
    "sequelize-typescript": "^2.1.1",
    "string-similarity": "^4.0.4",
    "utf-8-validate": "^6.0.3",
    "winston": "^3.8.1",
    "yaml": "^2.1.1",
    "zlib-sync": "^0.1.8"
  },
  "repository": "https://github.com/TheDeveloperDen/DevDenBot.git",
  "private": false,
  "devDependencies": {
    "@types/node": "^18.7.1",
    "@types/opentype.js": "^1.3.3",
    "@types/sequelize": "^4.28.14",
    "@types/sqlite3": "^3.1.8",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
